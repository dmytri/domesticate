{
  "name": "domesticate",
  "version": "3.0.3",
  "description": "test browser javascript in node",
  "repository": {
    "type": "git",
    "url": "git://github.com/dmytri/domesticate.git"
  },
  "keywords": [
    "dom",
    "test"
  ],
  "bugs": {
    "url": "https://github.com/dmytri/domesticate/issues"
  },
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/standard -v && /bin/sh test/test.sh"
  },
  "author": "Dmytri Kleiner <dk@trick.ca>",
  "license": "ISC",
  "dependencies": {
    "jsdom": "^6.5.1",
    "pkg-config": "^1.1.0"
  },
  "devDependencies": {
    "jquery": "^2.1.4",
    "mocha": "^2.3.3",
    "pre-commit": "^1.1.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-tools": "^0.13.3",
    "riot": "^2.2.4",
    "standard": "^5.3.1"
  },
  "domesticate": {
    "html": "<html><head></head><body><div id=\"test\">test</div><div id=\"trap\"></div></body></html>",
    "include": "test/testdom.html",
    "scripts": [
      {
        "src": "node_modules/riot/riot+compiler.min.js",
        "exports": [
          "riot"
        ]
      },
      {
        "src": "node_modules/react/dist/react.js",
        "exports": [
          "React"
        ]
      },
      {
        "src": "node_modules/react-dom/dist/react-dom.js",
        "exports": [
          "ReactDOM"
        ]
      }
    ]
  }
}
